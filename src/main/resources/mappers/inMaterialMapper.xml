<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  
 <mapper namespace="com.itwillbs.mappers.inMaterialMapper">
 	
 <!-- 입고 리스트 -->
 	<select id="getInMaterialListAll" resultType="com.itwillbs.domain.InMaterialVO">
 		select * from in_material
 		order by in_progress
 	</select>
 <!-- 입고 리스트 -->	
 
 
 <!-- in_id 입고번호 저장 -->
 	<insert id="registInId">
		update in_material set in_id = #{in_id}, in_progress = "입고완료"
		where order_id = #{order_id}
 	</insert>
 <!-- in_id 입고번호 저장 -->
 	
 	
 	
 	
 	
 	
 	
 		
 
 </mapper>
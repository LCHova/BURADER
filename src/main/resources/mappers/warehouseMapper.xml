<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
    <mapper namespace="com.itwillbs.mappers.warehouseMapper">
    
      <!--   창고 목록 조회  -->              
      <select id="getWarehouseList" resultType="com.itwillbs.domain.WarehouseVO">
        select * from warehouse
      </select>
   <!-- 창고 목록 조회  -->
   
      <insert id ="warehouseInsert">
       insert into warehouse (whs_id, whs_type, whs_tel, shelt_position, whs_status, whs_emp)
       values (#{whs_id} ,#{whs_type} ,#{whs_tel} ,#{shelt_position} ,#{whs_status} ,#{whs_emp})
      </insert>
   
   </mapper>